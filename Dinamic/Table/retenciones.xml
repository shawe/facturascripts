<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : retenciones.xml
    Author     : Cristo M. Estévez Hernández
    Description:
        Structure for the retenciones table.
-->
<table>
    <column>
        <name>codretencion</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>descripcion</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>porcentaje</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>retenciones_pkey</name>
        <type>PRIMARY KEY (codretencion)</type>
    </constraint>
</table>